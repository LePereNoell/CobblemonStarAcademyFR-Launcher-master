{
    "name": "cobblemon-star-academy-fr-launcher",
    "preductname": "cobblemon-star-academy-fr-launcher",
    "version": "1.0.2",
    "description": "Launcher Minecraft Cobblemon Star Academy FR",
    "main": "src/app.js",
    "author": {
        "name": "Le Père Noël"
    },
    "url": "https://www.cobblemonstaracademyfr.adkynet.fr/",
    "scripts": {
        "start": "cross-env-shell NODE_ENV=dev electron .",
